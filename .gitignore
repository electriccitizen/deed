# Ignore dependencies managed by Composer
/vendor/

# Ignore Drupal core and contributed modules/themes/profiles installed by Composer
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive configuration files
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/settings.ddev.php

# Ignore user-generated files and temporary directories
/web/sites/*/files/
/web/sites/simpletest/
/tmp/

# Ignore IDE-specific files (e.g., PhpStorm)
/.idea/
.DS_Store

# Ignore environment variables file
/.env

